		"utils:build": "rollup --config"
	},
	"author": "yuwen",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.24.3",
		"@babel/preset-env": "^7.24.3",
		"@rollup/plugin-babel": "^6.0.4",
		"@rollup/plugin-commonjs": "^25.0.7",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-typescript": "^11.1.6",
		"rollup": "^4.17.2",
		"typescript": "^5.4.5"
	}
